    "version": "0.2.0",
    "configurations": [
        {
            "name": "Service: dashboard",
            "type": "python",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "${workspaceFolder}/dashboard/Nemesis.py"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/dashboard/",
            "justMyCode": false,
            "env": {
                "LOG_LEVEL": "DEBUG",
                "ENVIRONMENT": "development",
                "PROMETHEUS_PORT": "18000",
                // "PYTHONASYNCIODEBUG": "1",
                "BETTER_EXCEPTIONS": "1",
                "FORCE_COLOR": "1",
            },
        }
    ]
}